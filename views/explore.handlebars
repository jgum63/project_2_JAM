  {{#if workouts.length}}
  <div class="col-md-6 project-list">
    <h3>Current Workouts:</h3>

    {{#each workouts as |workout|}}
    <div class="row mb-2">
      <div class="col-md-8">
        <h4>{{workout.name}}</h4>
        <h5>{{difficulty.difficulty}}</h5>
      </div>
    </div>
    {{/each}}
  </div>
  {{/if}}